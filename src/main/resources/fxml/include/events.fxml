<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import hu.vitamas.enotesz.*?>
<?import javafx.collections.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="567.0"
	prefWidth="952.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="hu.vitamas.enotesz.controller.EventsController">
	<children>
		<TitledPane fx:id="jumpToDayBox" animated="false"
			collapsible="false" layoutX="8.0" layoutY="8.0" prefHeight="126.0"
			prefWidth="352.0" text="Nap kiválasztása">
			<content>
				<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="120.0"
					prefWidth="350.0">
					<children>
						<DatePicker fx:id="jumpToDayInput" layoutX="18.0"
							layoutY="14.0" prefHeight="27.0" prefWidth="312.0" />
						<Button id="selectDay" fx:id="jumpToDayButton" layoutX="18.0"
							layoutY="52.0" mnemonicParsing="false" prefHeight="28.0"
							prefWidth="315.0" text="Ugrás a kiválasztott napra!">
							<styleClass>
								<String fx:value="btn" />
								<String fx:value="btn-light" />
								<String fx:value="btn-small" />
							</styleClass>
						</Button>
					</children>
				</AnchorPane>
			</content>
		</TitledPane>
		<TitledPane fx:id="calendarBox" animated="false"
			collapsible="false" layoutX="8.0" layoutY="149.0" prefHeight="345.0"
			prefWidth="352.0" text="Naptár">
			<content>
				<AnchorPane fx:id="calendarContainer" minHeight="0.0"
					minWidth="0.0" prefHeight="315.0" prefWidth="350.0">
					<children>
						<GridPane layoutY="0.0" prefHeight="20.0" prefWidth="350.0">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
									prefWidth="50.0" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="H"
									textAlignment="CENTER" />
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="K"
									textAlignment="CENTER" GridPane.columnIndex="1" />
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="Sze"
									textAlignment="CENTER" GridPane.columnIndex="2" />
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="Cs"
									textAlignment="CENTER" GridPane.columnIndex="3" />
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="P"
									textAlignment="CENTER" GridPane.columnIndex="4" />
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="Szo"
									textAlignment="CENTER" GridPane.columnIndex="5" />
								<Label alignment="CENTER" contentDisplay="CENTER"
									prefWidth="50.0" styleClass="calendar-header" text="V"
									textAlignment="CENTER" GridPane.columnIndex="6" />
							</children>
						</GridPane>
						<GridPane fx:id="calendar" layoutY="20.0" prefHeight="300.0"
							prefWidth="350.0">
						</GridPane>
					</children>
				</AnchorPane>
			</content>
		</TitledPane>
		<Button fx:id="prevMonth" layoutX="8.0" layoutY="500.0"
			mnemonicParsing="false" onMouseClicked="#prevMonthClick" prefHeight="25.0"
			prefWidth="145.0"
			style="-fx-background-radius: 5 0 0 5; -fx-border-radius: 5 0 0 5; -fx-border-width: 1 0 1 1;"
			text="&lt;&lt; Előző hónap">
			<styleClass>
				<String fx:value="btn" />
				<String fx:value="btn-light" />
				<String fx:value="btn-small" />
			</styleClass>
		</Button>
		<Button fx:id="nextMonth" layoutX="215.0" layoutY="500.0"
			mnemonicParsing="false" onMouseClicked="#nextMonthClick" prefHeight="25.0"
			prefWidth="145.0"
			style="-fx-background-radius: 0 5 5 0; -fx-border-radius: 0 5 5 0; -fx-border-width: 1 1 1 0;"
			text="Következő hónap &gt;&gt;">
			<styleClass>
				<String fx:value="btn" />
				<String fx:value="btn-light" />
				<String fx:value="btn-small" />
			</styleClass>
		</Button>
		<Button fx:id="currentMonth" layoutX="153.0" layoutY="500.0"
			mnemonicParsing="false" onMouseClicked="#currentMonthClick"
			prefHeight="25.0" prefWidth="62.0"
			style="-fx-background-radius: 0; -fx-border-radius: 0;">
			<graphic>
				<ImageView fitHeight="15" fitWidth="15">
					<image>
						<Image url="@../../images/fa-home-15px.png" />
					</image>
				</ImageView>
			</graphic>
			<styleClass>
				<String fx:value="btn" />
				<String fx:value="btn-light" />
				<String fx:value="btn-small" />
			</styleClass>
		</Button>
		<TitledPane animated="false" collapsible="false" layoutX="369.0"
			layoutY="8.0" prefHeight="487.0" prefWidth="569.0"
			text="Események a kijelölt napon">
			<content>
				<AnchorPane fx:id="eventsList" minHeight="0.0" minWidth="0.0"
					prefHeight="477.0" prefWidth="567.0" />
			</content>
		</TitledPane>
		<Button layoutX="814.0" layoutY="500.0" mnemonicParsing="false"
			onMouseClicked="#refresh" prefHeight="25.0" prefWidth="124.0"
			style="-fx-background-radius: 0 5 5 0; -fx-border-radius: 0 5 5 0; -fx-border-width: 1 1 1 0;"
			text="Lista frissítése">
			<graphic>
				<ImageView fitHeight="13" fitWidth="13">
					<image>
						<Image url="@../../images/fa-refresh-15px.png" />
					</image>
				</ImageView>
			</graphic>
			<styleClass>
				<String fx:value="btn" />
				<String fx:value="btn-light" />
				<String fx:value="btn-small" />
			</styleClass>
		</Button>
		<Button layoutX="637.0" layoutY="500.0" mnemonicParsing="false"
			onMouseClicked="#addEvent" prefHeight="25.0" prefWidth="177.0"
			style="-fx-background-radius: 5 0 0 5; -fx-border-radius: 5 0 0 5;"
			text="Új esemény létrehozása">
			<graphic>
				<ImageView fitHeight="13" fitWidth="13">
					<image>
						<Image url="@../../images/fa-plus-15px.png" />
					</image>
				</ImageView>
			</graphic>
			<styleClass>
				<String fx:value="btn" />
				<String fx:value="btn-light" />
				<String fx:value="btn-small" />
			</styleClass>
		</Button>
	</children>
</AnchorPane>
